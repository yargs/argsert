{
  "name": "argsert",
  "version": "1.0.0",
  "main": "lib/index.js",
  "repository": {},
  "files": [
    "lib"
  ],
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.23.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.23.0",
    "coveralls": "^2.11.16",
    "cross-env": "^4.0.0",
    "nyc": "^10.1.2",
    "semistandard": "^10.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "pretest": "semistandard",
    "test": "cross-env NODE_ENV=test nyc ava",
    "posttest": "yarn build",
    "preversion": "yarn test"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      ["env", { "targets": { "node": 4 } }]
    ],
    "env": {
      "test": {
        "plugins": ["istanbul"]
      }
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "check-coverage": true,
    "sourceMap": false,
    "instrument": false
  }
}
